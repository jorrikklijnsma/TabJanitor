{
  "name": "tab-janitor",
  "displayName": "TabJanitor",
  "description": "TabJanitor provides intuitive and flexible controls for managing your workspace's tabs. Designed to enhance productivity, TabJanitor offers advanced features that go beyond the default tab management capabilities. Whether you're dealing with a handful of files or navigating a sea of open tabs, TabJanitor makes it easy to stay organized and focused on your code. Start taking control of your workspace today with TabJanitor.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"v
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tabJanitor.closeTabsToLeft",
        "title": "Close Tabs to Left",
        "category": "TabJanitor"
      },
      {
        "command": "tabJanitor.closeAllButPinned",
        "title": "Close All But Pinned",
        "category": "TabJanitor"
      },
      {
        "command": "tabJanitor.closeAllExceptCurrentFileType",
        "title": "Close All Except Current File Type",
        "category": "TabJanitor"
      },
      {
        "command": "tabJanitor.closeAllTabsWithCurrentFileType",
        "title": "Close All Tabs With Current File Type",
        "category": "TabJanitor"
      },
      {
        "command": "tabJanitor.closeAllTabsFromSameDirectory",
        "title": "Close All Tabs From Same Directory",
        "category": "TabJanitor"
      },
      {
        "command": "tabJanitor.closeAllTabsWithoutChanges",
        "title": "Close All Tabs Without Changes",
        "category": "TabJanitor"
      }
    ],
    "menus": {
      "editor/title/context": [
        {
          "submenu": "TabJanitor",
          "group": "navigation@1"
        },
        {
          "command": "tabJanitor.closeTabsToLeft",
          "group": "TabJanitor@1",
          "submenu": "TabJanitor"
        },
        {
          "command": "tabJanitor.closeAllButPinned",
          "group": "TabJanitor@1",
          "submenu": "TabJanitor"
        },
        {
          "command": "tabJanitor.closeAllExceptCurrentFileType",
          "group": "TabJanitor@1",
          "submenu": "TabJanitor"
        },
        {
          "command": "tabJanitor.closeAllTabsWithCurrentFileType",
          "group": "TabJanitor@1",
          "submenu": "TabJanitor"
        },
        {
          "command": "tabJanitor.closeAllTabsFromSameDirectory",
          "group": "TabJanitor@1",
          "submenu": "TabJanitor"
        },
        {
          "command": "tabJanitor.closeAllTabsWithoutChanges",
          "group": "TabJanitor@1",
          "submenu": "TabJanitor"
        }
      ]
    },
    "submenus": [
      {
        "id": "TabJanitor",
        "label": "TabJanitor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}
